# GCP設定
GCP_PROJECT_ID=your-project-id
# GCP_REGION: Cloud Functionsのデプロイ先リージョン（Vertex AIエンドポイントは常にglobalを使用）
GCP_REGION=us-central1

# Firestore設定
FIRESTORE_DATABASE=(default)

# Cloud Storage設定
CDN_BASE_URL=https://storage.googleapis.com/your-bucket-name

# Cloud Storage設定
GCS_BUCKET_NAME=
# Gemini設定
GEMINI_MODEL=gemini-3-flash-preview
GEMINI_IMAGE_MODEL=gemini-2.5-flash-image
GEMINI_MAX_OUTPUT_TOKENS=32000
GEMINI_TEMPERATURE=1.0
GEMINI_THINKING_BUDGET_TOKENS=8192

# アプリケーション設定
DEBUG=true
LOG_LEVEL=DEBUG
IMAGE_GENERATION_FUNCTION_URL=https://generate-image-xxxxxxxx-uc.a.run.app
ANNOTATION_FUNCTION_URL=https://annotate-image-xxxxxxxx-uc.a.run.app

# Agent Engine設定
AGENT_ENGINE_ID=your-agent-engine-id
AGENT_ENGINE_LOCATION=us-central1

# CORS設定
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]
